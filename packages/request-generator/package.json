{
  "name": "@openapi-security-scanner/request-generator",
  "version": "0.0.0",
  "description": "Generate Postman collections and data sets for fuzzing your API",
  "author": "Ash Belmokadem <a.belmokadem@gmail.com>",
  "repository": "https://github.com/cloudchefs/openapi-security-scanner/tree/master/packages/request-generator",
  "keywords": [
    "cli"
  ],
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "tape": "teeouterr test-output.tap nyc tape 'test/**/*.js'",
    "xunit:generate": "cat test-output.tap | tap-xunit --package ${npm_package_name} > xunit.xml",
    "test": "npm run lint && npm run tape"
  },
  "license": "ISC",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "@openapi-security-scanner/fuzzers": "0.0.0",
    "@openapi-security-scanner/util": "0.0.0",
    "lodash.flattendeep": "^4.4.0",
    "sway": "^2.0.5"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "nyc": "^13.1.0",
    "standard": "^12.0.1",
    "tap-xunit": "^2.3.0",
    "teeouterr": "^1.2.0"
  }
}
